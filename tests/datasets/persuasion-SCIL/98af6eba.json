            "dialog_act": "",
            "comm_act_type": "",
            "link_to": "",
            "pos": "",
            "pos_count": "",
            "pos_origin": "",
            "topic": "",
            "polarity": "",
            "text": "Are you sure? How many dollars do you spend everyday on pointless or unnecessary items, such as a candy bar or fast food?"
        },
        {
            "mode": "chat",
            "speaker": "speaker2",
            "end_time": "",
            "turn_no": "11",
