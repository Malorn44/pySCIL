            "end_time": "",
            "turn_no": "13",
            "dialog_act": "",
            "comm_act_type": "",
            "link_to": "",
            "pos": "",
            "pos_count": "",
            "pos_origin": "",
            "topic": "",
            "polarity": "",
            "text": "I know international travel is expensive but having 14 percent of the donations going to overhead seems a bit high to me.  It should be no more than 10 percent max."
        },
        {
            "mode": "chat",
            "speaker": "speaker1",
