            "dialog_act": "",
            "comm_act_type": "",
            "link_to": "",
            "pos": "",
            "pos_count": "",
            "pos_origin": "",
            "topic": "",
            "polarity": "",
            "text": "There is a child right now 8000 miles away from you who is living daily with violence and no food. But we can help her."
        },
        {
            "mode": "chat",
            "speaker": "speaker2",
            "end_time": "",
