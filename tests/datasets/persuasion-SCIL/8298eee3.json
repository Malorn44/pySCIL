            "mode": "chat",
            "speaker": "speaker1",
            "end_time": "",
            "turn_no": "10",
            "dialog_act": "",
            "comm_act_type": "",
            "link_to": "",
            "pos": "",
            "pos_count": "",
            "pos_origin": "",
            "topic": "",
            "polarity": "",
