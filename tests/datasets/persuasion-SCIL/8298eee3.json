            "turn_no": "5",
            "dialog_act": "",
            "comm_act_type": "",
            "link_to": "",
            "pos": "",
            "pos_count": "",
            "pos_origin": "",
            "topic": "",
            "polarity": "",
            "text": "There were periods in my childhood when money was very short and we had our telephone or power cut off, but I always had a safe place to live and food to eat."
        },
        {
            "mode": "chat",
            "speaker": "speaker1",
