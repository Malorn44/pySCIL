        {
            "mode": "chat",
            "speaker": "speaker1",
            "end_time": "",
            "turn_no": "4",
            "dialog_act": "",
            "comm_act_type": "",
            "link_to": "",
            "pos": "",
            "pos_count": "",
            "pos_origin": "",
            "topic": "",
            "polarity": "",
            "text": "No, but I am a huge advocate for the alienable rights of children and decided to spread the word about the organization in order to gain more traction for the work."
        },
